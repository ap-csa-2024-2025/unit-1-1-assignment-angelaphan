public class Printing
{

    public static void main(String[] args)
    {
        // int, double, boolean, string
        int myAge = 80;
        double price = 4.99; //decimal
        boolean isGood;
        isGood = false;
        String name = "giggity"; // capital for string
        System.out.print("Hello my name is " + name);

    }
}